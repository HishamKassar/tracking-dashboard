{
  "trailingComma": "none",
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@storybook/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ]
}
